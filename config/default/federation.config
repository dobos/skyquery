<configuration>
  <configSections>
    <sectionGroup name="Jhu.Graywulf">
      <section name="Format" type="System.Configuration.AppSettingsSection"/>
      <sectionGroup name="Registry">
        <section name="Federation" type="System.Configuration.AppSettingsSection"/>
      </sectionGroup>
      <section name="Web.UI" type="System.Configuration.AppSettingsSection"/>
    </sectionGroup>
  </configSections>
  
  <Jhu.Graywulf>
    <Format>
      <add key="FileFormatFactory" value="Jhu.SkyQuery.Format.SkyQueryFileFormatFactory, Jhu.SkyQuery.Format" />
      <add key="StreamFactory" value="Jhu.SkyQuery.Format.SkyQueryStreamFactory, Jhu.SkyQuery.Format" />
    </Format>
    <Registry>
      <Federation>
        <add key="FederationName" value="Graywulf.VoServices.SkyQuery"/>
        <add key="QueryFactory" value="Jhu.SkyQuery.Jobs.Query.XMatchQueryFactory, Jhu.SkyQuery" />
      </Federation>
    </Registry>
    <Web.UI>
      <add key="ExportDir" value="\\gwen1\SkyQueryOutput"/>
    </Web.UI>
  </Jhu.Graywulf>
</configuration>